{% extends 'base.html.twig' %}
{% block title %}Boryspil/Aero/Union Admin{% endblock %}
{% block stylesheets %}
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet"
        type="text/css"
        href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
  {{ encore_entry_link_tags('admin') }}
{% endblock %}
{% block body %}
  <div class="wrapper">
    {% include 'admin/elements/admin_nav.html.twig' %}
    <div class="main-panel">
      <!-- Navbar -->
      {% include 'admin/elements/admin_nav.html.twig' %}
      <!-- End Navbar -->
      <div class="content">
        <div class="container-fluid">
          <img id="open-sidebar" style="display: none; height: 80px;" src="{{ asset('content/sidebar.svg') }}" >
          {% block content %}
          {% endblock %}
        </div>
      </div>
      <footer class="footer">
        <div class="container-fluid">
          <nav class="float-left">
            <ul>
              <li>
                <a href="https://www.creative-tim.com">
                  Octowise &copy;
                  <script>
                    document.write(new Date().getFullYear())
                  </script>
                </a>
              </li>
            </ul>
          </nav>
        </div>
      </footer>
    </div>
  </div>
{% endblock %}
{% block javascripts %}
  {{ encore_entry_script_tags('admin') }}
{% endblock %}